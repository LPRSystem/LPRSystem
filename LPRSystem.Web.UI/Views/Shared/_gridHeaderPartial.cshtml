<div class="card-header card-header-padding">
    <div class="float-left">
        <span id="addBtn" class="padding-rigt custom-cursor btn btn-success">
            <i class="fa fa-plus-circle" title="add"></i>
        </span>
        <span id="editBtn" class="padding-rigt custom-cursor btn btn-primary">
            <i class="fa fa-pencil" title="edit"></i>
        </span>
        <span id="deleteBtn" class="padding-rigt custom-cursor btn btn-danger"><i class="fa fa-trash" title="delete"></i></span>
        <span id="copyBtn" class="padding-rigt custom-cursor btn btn-warning"><i class="fa fa-plus-square-o " title="copy"></i></span>
        <span id="permissionBtn" class="custom-cursor permission-hidden"><i class="fa fa-key" title="manage permissions"></i></span>
        <span id="campaignBtn" class="custom-cursor permission-hidden"><i class="fa fa-key" title="manage campaign"></i></span>
        <span id="dealerBtn" class="custom-cursor permission-hidden"><i class="fa fa-key" title="manage dealer"></i></span>
    </div>
    
    <div class="float-right">
        <span id="pdfBtn" class="custom-cursor padding-right">

            <input id="navbar-search-input" type="search" placeholder="Search Docs" class="ds-input" autocomplete="off" spellcheck="false" role="combobox" aria-autocomplete="list" aria-expanded="false" aria-owns="algolia-autocomplete-listbox-2" dir="auto" style="position: relative; vertical-align: top; background-color: transparent;">

            <i class="fa-solid fa-file-pdf permission-hidden" title="pdf" id="tenantDealerDetailInfoPDF"></i>
        </span>
        @* <input type="text" class="form-control" id="Name" placeholder="Enter your Name" autofocus required /> *@

        <span id="importBtn" class="custom-cursor padding-right btn btn-secondary permission-hidden">
            <i class="fa fa-upload permission-hidden" title="import"></i>
        </span>
        <input class="form-control" type="text" placeholder="search here" style="margin-right:5px" id="inputSearch">
        <span id="exportBtn" class="custom-cursor btn btn-info" data-toggle="dropdown">
            <i class="fa fa-download" title="export"></i>
        </span>
        <div class="dropdown-menu dropdown-menu-right">
            <a class="dropdown-item" href="#" id="exportTemplate">Export Template</a>
            <a class="dropdown-item" href="#" id="exportWithGridData">Export with Grid Data</a>
            <a class="dropdown-item" href="#" id="exportWithOriginalData">Export with Original Data</a>
        </div>
    </div>
</div>


